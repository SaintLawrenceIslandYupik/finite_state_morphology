!================
!
Multichar_Symbols
!
!================
%^     ! morpheme boundary

%{g%}  ! for digraph -gh
%{k%}  ! for digraph -kw
%{t%}  ! for -te

!-----------
! allomorphs 
!-----------
%{E%}  ! for (e)
%{G%}  ! for (g) 

!---------------------------
! morphophonological symbols 
!---------------------------
%{.w.%}   ! drop weak -gh
%{.c.%}   ! drop uvular (-gh) between single vowels
%{.f.%}   ! drop and hop final -e
%{.fL.%}  ! drop and hop final -e if root exceeds two syllables
%{.sf.%}  ! drop and hop semi-final -e
%{.m.%}   ! drop root-final consonants
%{.at.%}  ! modify root-final -te 

!-----------------------------
! morphological tags for nouns
!-----------------------------
[N.N]  ! noun-elaborating derivational suffix
[N.V]  ! verbalizing derivational suffix

[Sg]  ! singular
[Pl]  ! plural 
[Du]  ! dual

[Abs]      ! absolutive case
[Abl.Mod]  ! ablative-modalis case
[Loc]      ! locative case 
[All]      ! allative case
[Per]      ! perlative case
[Equ]      ! equative case

!-----------------------------
! morphological tags for verbs
!-----------------------------
[V.V]    ! verb-elaborating derivational suffix
[V.N]    ! nominalizing derivational suffix

[Intr]  ! intransitive (valency)
[Trns]  ! transitive (valency)

[Ind]   ! indicative mood


!===========
!
LEXICON Root
!
!===========
NounRootI;     ! ends in -a, -i, -u, -aa, -ii, -uu
NounRootII;    ! ends in -g, -w, -ghw
NounRootIII;   ! ends in weak -gh
NounRootIV;    ! ends in marked strong -gh
NounRootV;     ! ends in strong -gh
NounRootVI;    ! ends in -te
NounRootVII;   ! has semi-final -e that can drop and hop
NounRootVIII;  ! has semi-final -e that can drop but not hop
NounRootIX;    ! has final -e that can drop and hop
NounRootX;     ! has final -e that can drop but not hop 

VerbRootI;     ! ends in -a, -i, -u
VerbRootII;    ! has final -e
VerbRootIII;   ! ends in -g, -w, -ghw
VerbRootIV;    ! ends in -agh, -igh, -ugh or -egh where e cannot be drop
VerbRootV;     ! ends in -te
VerbRootVI;    ! has semi-final -e that can drop and hop
VerbRootVII;   ! has semi-final -e that can drop but not hop

Particle;


!================
LEXICON Particle
!================

elngaatall  #;  ! very much
quunpeng    #;  ! always
unaami      #;  ! tomorrow
qayughllak  #;  ! because

!=====================================================
LEXICON NounSuffixI
!  noun-elaborating [NN] and verbalizing [NV] suffixes
!  when suffixing to noun roots that end in -a, -i, -u
!=====================================================

                             NounInflI;

-ghllag[N.N]:%^ghllag        NounSuffixII;   ! big N; lots of N
-ghrugllag[N.N]:%^ghrugllag  NounSuffixII;   ! big N, huge N
-krag[N.N]:%^krag            NounSuffixII;   ! fancy piece of N

-kagh[N.N]:%^kagh            NounSuffixIII;  ! raw material for N
-qragh[N.N]:%^qragh          NounSuffixIII;  ! small N; thing like N

-leg[N.N]:%^leg              NounSuffixVIII; ! one having lots of N

-u[N.V]:%^ngu                VerbSuffixI;    ! to be N

-ghhaleg[N.V]:%^ghhaleg                   #; ! how V! (exclamation)

-mii[N.N]:%^mii              NounSuffixI;    ! resident of N

-ngllagh[N.V]:%^ngllagh      VerbSuffixIV;   ! to make N

-squq[N.N]:%^squ%{g%}h       NounSuffixIII;  ! little N, small N
-squghhaq[N.N]:%^squghhagh   NounSuffixIV;   ! little N, small N

-lgu[N.V]:%^lgu              VerbSuffixI;    ! to have N

-liigh[N.V]:%^liigh          VerbSuffixIV;   ! to prepare N, to cook N

-niigh[N.V]:%^niigh          VerbSuffixIV;   ! to hunt for N, to search for n, to ask for N, to work with N

-te[N.V]:%^te                VerbSuffixV;   ! to go to N, to catch N

-tugh[N.V]:%^tugh            VerbSuffixIV;   ! to eat N, to use N





